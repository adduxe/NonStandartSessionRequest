<configuration>
  <appSettings>
    <add key="serilog:using:Seq" value="Serilog.Sinks.Seq" />
    <add key="serilog:write-to:Seq.serverUrl" value="http://oweb7-vm.usc.edu:5341/webdev" />
    <add key="serilog:enrich:with-property:Application" value="USC.RNR.NonStandardSessionRequest" />
    <!--<add key="serilog:minimum-level" value="Debug" />-->

    <add key="DataApiUrl" value="https://oweb7-vm.usc.edu/dataapi" />
    <!-- add key="DataApiUrl" value="http://localhost:1615/dataapi" /-->
    <add key="UvApiUrl" value="https://oweb7-vm.usc.edu/uvapi" />
